{
  "expo": {
    "name": "ReCare",
    "slug": "ckd",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/icon.png",
    "scheme": ["ckd"],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "assets/images/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aditya210399.CKD"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aditya210399.CKD"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-notifications",
        {
          "sounds": ["assets/sound/mysoundfile.mp3"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "d8da9c21-0ca3-4f2c-94cf-bd71a8d3f0c7"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "aditya210399"
  }
}
